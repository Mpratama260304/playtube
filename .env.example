APP_NAME=PlayTube
APP_ENV=local
APP_KEY=
APP_DEBUG=true
APP_URL=http://localhost

# GitHub Codespaces Configuration:
# Replace <CODESPACE_NAME> with your actual codespace name (e.g., urban-space-xyzabc)
# Find it with: echo $CODESPACE_NAME
#
# APP_URL=https://<CODESPACE_NAME>-8000.app.github.dev
# ASSET_URL=https://<CODESPACE_NAME>-8000.app.github.dev
#
# NOTE: ASSET_URL should point to PORT 8000 (Laravel), NOT port 5173 (Vite dev server).
# The Vite dev server URL is handled automatically by vite.config.js.
# Setting ASSET_URL to the 5173 port will cause MIME type and CORS errors!

APP_LOCALE=en
APP_FALLBACK_LOCALE=en
APP_FAKER_LOCALE=en_US

APP_MAINTENANCE_DRIVER=file
# APP_MAINTENANCE_STORE=database

# PHP_CLI_SERVER_WORKERS=4

BCRYPT_ROUNDS=12

LOG_CHANNEL=stack
LOG_STACK=single
LOG_DEPRECATIONS_CHANNEL=null
LOG_LEVEL=debug

DB_CONNECTION=sqlite
# DB_HOST=127.0.0.1
# DB_PORT=3306
# DB_DATABASE=laravel
# DB_USERNAME=root
# DB_PASSWORD=

# Session Configuration
# For HTTPS proxy (Cloudflare, trycloudflare, ngrok), use:
#   SESSION_DRIVER=file
#   SESSION_DOMAIN=   (leave empty for dynamic domains)
#   SESSION_SECURE_COOKIE=false (auto-detected by middleware)
SESSION_DRIVER=file
SESSION_LIFETIME=120
SESSION_ENCRYPT=false
SESSION_PATH=/
SESSION_DOMAIN=
SESSION_SECURE_COOKIE=false

BROADCAST_CONNECTION=log
FILESYSTEM_DISK=local
QUEUE_CONNECTION=database

CACHE_STORE=database
# CACHE_PREFIX=

MEMCACHED_HOST=127.0.0.1

REDIS_CLIENT=phpredis
REDIS_HOST=127.0.0.1
REDIS_PASSWORD=null
REDIS_PORT=6379

MAIL_MAILER=log
MAIL_SCHEME=null
MAIL_HOST=127.0.0.1
MAIL_PORT=2525
MAIL_USERNAME=null
MAIL_PASSWORD=null
MAIL_FROM_ADDRESS="hello@example.com"
MAIL_FROM_NAME="${APP_NAME}"

AWS_ACCESS_KEY_ID=
AWS_SECRET_ACCESS_KEY=
AWS_DEFAULT_REGION=us-east-1
AWS_BUCKET=
AWS_USE_PATH_STYLE_ENDPOINT=false

VITE_APP_NAME="${APP_NAME}"

# GitHub Codespaces Vite Dev Server
# Set this to the forwarded HTTPS URL for port 5173 in Codespaces:
# VITE_DEV_SERVER_URL=https://<CODESPACE_NAME>-5173.app.github.dev
